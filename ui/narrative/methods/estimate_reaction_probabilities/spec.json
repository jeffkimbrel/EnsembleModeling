{
  "name" : "Estimate Reaction Probabilities",
  "ver" : "0.0.1",
  "authors" : ["patrikd","kimbrel1","chenry"],
  "contact" : "http://kbase.us/contact-us/",
  "visble" : true,
  "categories" : ["active","annotation","metabolic_modeling"],
  "widgets" : {
    "input" : null,
    "output" : ""
  },
  "parameters" : [
     {
       "id" : "genome",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "" ],
       "field_type" : "text",
       "text_options" : {
         "valid_ws_types" : [ "KBaseGenomes.Genome" ]
       }
     },
     {
        "id" : "annotations_to_merge",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "textsubdata",
        "textsubdata_options" : {
           "allow_custom" : false,
           "show_src_obj" : false,
           "subdata_selection" : {
              "path_to_subdata" : [
                 "ontology_events"
              ],
              "parameter_id" : "genome",
              "subdata_included" : [
                 "ontology_events/[*]/id",
                 "ontology_events/[*]/timestamp",
                 "ontology_events/[*]/description",
                 "ontology_events/[*]/method"
              ],
              "selection_description": ["id", "timestamp", "description", "method"],
              "description_template" : "<strong>{{id}}</strong> ontology terms from <strong>{{method}}</strong> added <strong>{{timestamp}}</strong>",
              "selection_id" : "description"
           },
           "multiselection" : true
        },
        "text_options" : {
           "valid_ws_types" : []
        }
     },
     {
        "id" : "lineage",
        "field_type" : "dropdown",
        "allow_multiple" : false,
        "optional" : false,
        "advanced" : false,
        "default_values" : [
           "bacteria"
        ],
        "dropdown_options":{
            "options": [{
                "value": "bacteria",
                "display": "Bacteria"
            }, {
                "value": "fungi",
                "display": "Fungi"
            }, {
                "value": "archaea",
                "display": "Archaea"
             }, {
                "value": "metazoan",
                "display": "Metazoan"
            }, {
                "value": "viridiplantae",
                "display": "Viridiplantae"
            }, {
                "value": "other",
                "display": "Other"
           }]
        }
     },
     {
       "id" : "description",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "Probabilistic annotations" ],
       "field_type" : "text",
       "text_options" : {}
     },
     {
       "id" : "output_name",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "" ],
       "field_type" : "text",
       "text_options" : {
         "valid_ws_types" : [ "KBaseGenomes.Genome" ],
         "is_output_name" : true
       }
     }
  ],
  "behavior" : {
    "service-mapping": {
       "url": "",
       "name": "EnsembleModeling",
       "method": "estimate_reaction_probabilities",
       "input_mapping": [
           {
               "narrative_system_variable": "workspace",
               "target_property": "workspace_name"
           },
           {
               "target_property" : "genome",
               "input_parameter" : "genome",
               "target_type_transform": "resolved-ref"
           },
           {
               "target_property" : "annotations_to_merge",
               "input_parameter" : "annotations_to_merge"
           },
           {
               "target_property" : "lineage",
               "input_parameter" : "lineage"
           },
           {
               "target_property" : "description",
               "input_parameter" : "description"
           },
           {
               "target_property" : "output_name",
               "input_parameter" : "output_name"
           }
       ],
      "output_mapping" : [
     {
             "narrative_system_variable": "workspace",
             "target_property": "wsName"
           },
           {
             "service_method_output_path": [0,"obj_ref"],
             "target_property": "obj_ref",
             "target_type_transform": "resolved-ref"
           },
           {
               "service_method_output_path": [0, "report_name"],
               "target_property": "report_name"
           },
           {
               "service_method_output_path": [0, "report_ref"],
               "target_property": "report_ref"
           },
           {
               "constant_value": "16",
               "target_property": "report_window_line_height"
           }
      ]
    }
  },
  "job_id_output_field": "docker"
}